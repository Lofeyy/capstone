<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Gradient background -->
    <item>
        <shape android:shape="rectangle">

            <gradient
                android:type="linear"
                android:startColor="#2D305B"

                android:endColor="#2D305B"
                android:angle="270"
                />
            <corners android:radius="5dp" />
        </shape>
    </item>


</layer-list>
